<h2>Sign In</h2>
<div class="form">
  <%= form_for @user, url: session_path, method: :post do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Submit" %>

  <% end %>
</div>

<div class="nav">
  <h2><%= link_to "Home Page", main_index_path, class: "link" %>
</div>
