<h2>Sign Up</h2>
<div class="form">
  <%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Submit" %>
  <% end %>
</div>

<div class="nav">
  <h2><%= link_to "Home Page", main_index_path, class: "link" %></h2>
</div>
